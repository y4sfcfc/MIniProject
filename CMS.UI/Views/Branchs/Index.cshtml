@model List<BranchVM>
<div class="d-flex gap-3 pt-5">

    @foreach (var item in Model)
    {
        <div class="card " style="width: 15rem;">
            <div class="card-body d-flex justify-content-between align-items-center">
                @* <img width="100%" height="200px" src="@Url.Content($"~/{item.Thumbnail}")" alt="Alternate Text" /> *@
                <a class="text-decoration-none" asp-controller="Products" asp-action="Index" asp-route-branchId="@item.Id"> <h5 class="card-title">@item.Name</h5> </a>
            </div>
        </div>
    }

</div>